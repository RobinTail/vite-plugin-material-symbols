{
  "name": "vite-plugin-material-symbols",
  "version": "0.2.0",
  "description": "Selective loading of Material Symbols for production",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/RobinTail/vite-plugin-material-symbols.git"
  },
  "author": "Robin Tail <robin_tail@me.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "lint": "bun run biome check",
    "prebuild": "tsc --noEmit",
    "build": "tsdown",
    "postbuild": "attw --pack --profile esm-only",
    "mdfix": "bunx --bun prettier *.md --write",
    "version": "bun run version.ts",
    "prepublishOnly": "bun lint && bun test && bun run build"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "*.md"
  ],
  "peerDependencies": {
    "vite": "^6.0.0"
  },
  "dependencies": {
    "esquery": "^1.6.0"
  },
  "devDependencies": {
    "@arethetypeswrong/cli": "^0.17.0",
    "@biomejs/biome": "1.9.4",
    "@tsconfig/bun": "^1.0.7",
    "@types/bun": "^1.1.14",
    "@types/esquery": "^1.5.4",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@types/semver": "^7.5.8",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "semver": "^7.6.3",
    "tsdown": "^0.3.1",
    "typescript": "^5.7.2"
  }
}
